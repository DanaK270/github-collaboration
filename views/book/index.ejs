<!-- display books-->
<div class="container text-center">
  <div class="row"></div>
    <% books.forEach(book => {%>
      <div class="card col-4">
        <h5><%= book.title %></h5>
        <h5><%= book.price %>$</h5>
        <h5>Book Category: <%= book.category %></h5>
        <h5>Book Condition <%= book.condition %></h5>
      </div> 
    <%})%>
  </div>
</div>   
